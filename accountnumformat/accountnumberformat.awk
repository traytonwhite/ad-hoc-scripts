#!/usr/bin/gawk -f

BEGIN {
        FS="";
        OFS="";
}
{
        if ( NF == 1 ) {
                print "0000-000-0" $0;
        }
        if ( NF == 2 ) {
                print "0000-000-" $0;
        }
        if ( NF == 3 ) {
                print "0000-00" $1 "-" $2 $3;
        }
        if ( NF == 4 ) {
                print "0000-0" $1 $2 "-" $3 $4;
        }
        if ( NF == 5 ) {
                print "0000-" $1 $2 $3 "-" $4 $5;
        }
        if ( NF == 6 ) {
                print "000" $1 "-" $2 $3 $4 "-" $5 $6;
        }
        if ( NF == 7 ) {
                print "00" $1 $2 "-" $3 $4 $5 "-" $6 $7;
        }
        if ( NF == 8 ) {
                print "0" $1 $2 $3 "-" $4 $5 $6 "-" $7 $8;
        }
        if ( NF == 9 ) {
                print $1 $2 $3 $4 "-" $5 $6 $7 "-" $8 $9;
        }
}
